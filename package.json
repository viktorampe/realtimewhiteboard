    "@ngrx/store": "7.0.0-beta.0",
    "@nrwl/nx": "7.1.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "ngrx-undo": "^6.0.0",
    "ngx-image-cropper": "^1.3.5",
    "rxjs": "^6.3.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",

    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.6",
    "@types/node": "^8.10.59",
    "@types/uuid": "^3.4.4",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.8.1",
    "dotenv": "6.2.0",
    "jasmine-marbles": "^0.4.0",
    "jest": "^24.9.0",
    "jest-junit": "^6.4.0",
    "jest-preset-angular": "^7.1.1",
    "ng-packagr": "^4.7.1",
    "prettier": "1.16.4",
    "rxjs-marbles": "^4.3.1",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3"
  }
}

{
  "name": "campus",
  "version": "1.0.5",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --passWithNoTests",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint all-apps-files && ng lint all-libs-files",
    "e2e:polpo:development": "cross-env CYPRESS_ENVIRONMENT=development ng e2e polpo-classroom-web-e2e --watch",
    "e2e:polpo:staging": "node ./node_modules/cross-env/dist/bin/cross-env CYPRESS_ENVIRONMENT=staging node --max_old_space_size=12000 ./node_modules/@angular/cli/bin/ng e2e polpo-classroom-web-e2e --headless --configuration staging",
    "e2e:kabas:development": "cross-env CYPRESS_ENVIRONMENT=development ng e2e kabas-web --watch",
    "e2e:kabas:staging": "node ./node_modules/cross-env/dist/bin/cross-env CYPRESS_ENVIRONMENT=staging node --max_old_space_size=12000 ./node_modules/@angular/cli/bin/ng e2e kabas-web-e2e --headless --configuration staging",
    "e2e:timeline-editor:development": "cross-env CYPRESS_ENVIRONMENT=development ng e2e timeline-editor-e2e --watch",
    "e2e:timeline-editor:staging": "node ./node_modules/cross-env/dist/bin/cross-env CYPRESS_ENVIRONMENT=staging node --max_old_space_size=12000 ./node_modules/@angular/cli/bin/ng e2e timeline-editor-e2e --headless --configuration staging",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "build:polpo:staging": "ng build polpo-classroom-web -c staging --outputPath dist/polpo-web-staging",
    "build:polpo:production": "ng build polpo-classroom-web -c production --outputPath dist/polpo-web-production",
    "build:kabas:staging": "ng build kabas-web -c staging --outputPath dist/kabas-web-staging",
    "build:kabas:production": "ng build kabas-web -c production --outputPath dist/kabas-web-production",
    "build:elements-ink": "ng build elements-ink",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "nx": "nx"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^8.3.21",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/elements": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^7.1.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@diekeure/polpo-api-angular-sdk": "^2.1.22",
    "@ngrx/effects": "8.6.0",
    "@ngrx/entity": "8.6.0",
    "@ngrx/router-store": "8.6.0",
    "@ngrx/store": "8.6.0",
    "@nrwl/angular": "8.9.0",
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "hammerjs": "^2.0.8",
    "ng-bullet": "^1.0.3",
    "ngrx-undo": "^6.0.1",
    "ngx-build-plus": "^7.8.3",
    "ngx-image-cropper": "^1.3.5",
    "rxjs": "^6.5.3",
    "weak": "^1.0.1",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.3",
    "@angular-devkit/build-ng-packagr": "~0.803.3",
    "@angular/cli": "8.3.3",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@diekeure/angular-schematics": "0.1.12",
    "@ngrx/schematics": "8.6.0",
    "@ngrx/store-devtools": "8.6.0",
    "@nrwl/cypress": "8.9.0",

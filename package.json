    "@nrwl/schematics": "7.1.1",
    "@types/jest": "^23.3.8",
    "@types/jquery": "3.3.6",
    "@types/node": "~8.9.4",
    "@types/uuid": "^3.4.4",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.5",
    "husky": "^1.0.0-rc.13",
    "jasmine-marbles": "^0.4.0",
    "jest": "^23.6.0",
    "jest-junit": "^6.4.0",
    "jest-preset-angular": "6.0.1",
    "ng-packagr": "^4.7.1",
    "ngrx-store-freeze": "0.2.4",
    "prettier": "1.15.2",
    "rxjs-marbles": "^4.3.1",
    "ts-node": "~5.0.1",
